@{
}
@model List<Aluno>
<div>
    <table class="table">
        <thead>
            <tr>
                <th scope="col">Matrícula</th>
                <th scope="col">Nome</th>
                <th scope="col">Cpf</th>
            </tr>
        </thead>
        <tbody>
            @if (Model != null && Model.Any())
            {
                foreach (Aluno aluno in Model)
                {
                    <tr>
                        <td>@aluno.Matricula</td>
                        <td>@aluno.Nome</td>
                        <td>@aluno.Cpf</td>
                    </tr>
                }
            }
        
        </tbody>
    </table>
</div>